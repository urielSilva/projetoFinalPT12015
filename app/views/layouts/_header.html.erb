                        <li><%= link_to 'Cargos', jobs_path %></li>
                        <li><%= link_to 'Funções', functions_path %></li>
                        <li><%= link_to 'Funcionalidades', job_has_functions_path %></li>

                    <% end %>
                <li><%=link_to "Log out", logout_path, method: "delete" %></li>
            <% end %>
        </ul>
    </div>
</div>