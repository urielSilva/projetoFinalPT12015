<p id="notice"><%= notice %></p>

<h1>Listing Job Has Functions</h1>

<table>
  <thead>
    <tr>
      <th>Job</th>
      <th>Function</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_has_functions.each do |job_has_function| %>
      <tr>
        <td><%= job_has_function.job_id %></td>
        <td><%= job_has_function.function_id %></td>
        <td><%= link_to 'Show', job_has_function %></td>
        <td><%= link_to 'Edit', edit_job_has_function_path(job_has_function) %></td>
        <td><%= link_to 'Destroy', job_has_function, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job has function', new_job_has_function_path %>
