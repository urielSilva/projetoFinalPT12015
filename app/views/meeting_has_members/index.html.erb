<p id="notice"><%= notice %></p>

<h1>Listing Meeting Has Members</h1>

<table>
  <thead>
    <tr>
      <th>Meeting</th>
      <th>Member</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meeting_has_members.each do |meeting_has_member| %>
      <tr>
        <td><%= meeting_has_member.pega_reuniao meeting_has_member.meeting_id %></td>
        <td><%= meeting_has_member.pega_membro meeting_has_member.member_id %></td>
        <td><%= link_to 'Show', meeting_has_member %></td>
        <td><%= link_to 'Edit', edit_meeting_has_member_path(meeting_has_member) %></td>
        <td><%= link_to 'Destroy', meeting_has_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

